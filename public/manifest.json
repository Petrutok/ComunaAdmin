{
  "id": "digital-primaria-app",
  "name": "Primăria Digitală",
  "short_name": "Primăria",
  "description": "Platformă digitală pentru cetățeni - Servicii online, plăți, cereri și informații utile",
  "start_url": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone"],
  "theme_color": "#1e293b",
  "background_color": "#1e293b",
  "orientation": "any",
  "dir": "ltr",
  "lang": "ro-RO",
  "scope": "/",
  "categories": ["government", "utilities", "productivity"],
  "prefer_related_applications": false,
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  
  "scope_extensions": [
    {
      "origin": "*.primaria.digital"
    },
    {
      "origin": "https://api.primaria.digital"
    }
  ],
  
  "widgets": [
    {
      "name": "Anunțuri Recente",
      "description": "Vezi ultimele anunțuri ale primăriei",
      "tag": "announcements",
      "template": "announcements-widget",
      "type": "application/json+widget",
      "data": "/api/widget/announcements",
      "screenshots": [
        {
          "src": "/widget-announcements.png",
          "sizes": "600x400",
          "label": "Widget anunțuri"
        }
      ]
    },
    {
      "name": "Scadențe Taxe",
      "description": "Vezi scadențele pentru plata taxelor",
      "tag": "taxes",
      "template": "taxes-widget",
      "type": "application/json+widget",
      "data": "/api/widget/taxes",
      "screenshots": [
        {
          "src": "/widget-taxes.png",
          "sizes": "600x400",
          "label": "Widget taxe"
        }
      ]
    },
    {
      "name": "Status Cereri",
      "description": "Verifică statusul cererilor tale",
      "tag": "requests",
      "template": "requests-widget",
      "type": "application/json+widget",
      "data": "/api/widget/requests",
      "screenshots": [
        {
          "src": "/widget-requests.png",
          "sizes": "600x400",
          "label": "Widget cereri"
        }
      ]
    }
  ],
  
