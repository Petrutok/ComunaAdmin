  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*", "application/pdf"]
        }
      ]
    }
  },
  
  "file_handlers": [
    {
      "action": "/open-pdf",
      "accept": {
        "application/pdf": [".pdf"]
      }
    }
  ],
  
  "handle_links": "preferred",
  
  "protocol_handlers": [
    {
      "protocol": "web+primaria",
      "url": "/protocol?url=%s"
    }
  ],
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=digital.primaria.app",
      "id": "primaria-digitala"

      
    }
  ]
